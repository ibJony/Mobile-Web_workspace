<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jon.app" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Splash"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:theme="@style/GreenActionBarTheme"
            android:name="com.jon.app.MainActivity"
            android:label="@string/app_name"
            >

            <intent-filter>
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>

        </activity>
        <activity android:name="com.jon.app.Option2"></activity>
        <activity android:name="com.jon.app.Option1"></activity>
        <activity android:name="com.jon.app.Option3"></activity>
        <activity android:name="com.jon.app.Option4"></activity>
        <activity android:name="com.jon.app.PollStats" android:launchMode="singleInstance"></activity>
        <activity android:name="com.jon.app.Pollcomments"></activity>
        <activity android:name="com.jon.app.PollCodeActivity"></activity>
        <activity android:name="com.jon.app.ActivePolls" android:theme="@style/GreenActionBarTheme"></activity>
        <activity android:name=".MyDefault"></activity>
        <activity android:name=".OptChart" android:launchMode="singleInstance"></activity>
        <activity android:name=".RegisterActivity"></activity>
        <activity android:name="org.achartengine.GraphicalActivity" android:launchMode="singleInstance"></activity>

    </application>

</manifest>
